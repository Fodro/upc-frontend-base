{
	"compilerOptions": {
		"target": "es5",
		"lib": [
			"dom",
			"dom.iterable",
			"esnext"
		],
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noFallthroughCasesInSwitch": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"jsx": "react-jsx",
	//---------------------------------------------
	// Modified (start)
	//---------------------------------------------
		"baseUrl": "src",
		"types": [
			"./node_modules/@emotion/react/types/css-prop"
		]
	},
	"include": [
		"src"
	]
	//---------------------------------------------
	// Modified (end)
	//---------------------------------------------
}